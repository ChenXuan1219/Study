<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>chenxuan.html</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>


    <servlet>
        <servlet-name>HttpServle_Test</servlet-name>
        <servlet-class>com.pinkman.HttpServlet_Test</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HttpServle_Test</servlet-name>
        <url-pattern>/HttpServle_Test</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Servlet_Test</servlet-name>
        <servlet-class>com.pinkman.Servlet_Test</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet_Test</servlet-name>
        <url-pattern>/Servlet_Test</url-pattern>
    </servlet-mapping>







</web-app>